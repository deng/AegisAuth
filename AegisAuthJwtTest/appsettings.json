{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AuthSetting": {
    "JwtTokenKey": "your-super-secret-jwt-key-here-at-least-32-characters-long",
